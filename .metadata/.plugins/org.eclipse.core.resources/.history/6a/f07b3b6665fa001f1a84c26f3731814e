package modelo.BoletinInterfaces;

public abstract class Animal {

	private String nombreespecie;
	private String dondehabita;
	protected Familia tipofamilia;
	private Familia [] animalescome;
	private Familia [] animalescomido;
	
	abstract boolean atacar ();
	abstract boolean huir ();
	
	public enum Familia {
		RAPAZ,LEON,ROEDOR,SERPIENTE
	}
}
