package ejemplos;

public class Persona {

	int contador;
	int identificador;
	String nombre;
	int edad;
	
	public Persona(int contador, int identificador, String nombre, int edad) {
		super();
		this.contador = contador;
		this.identificador = identificador;
		this.nombre = nombre;
		this.edad = edad;
	}
	
	
}
