package ejemplos;

public class Persona {

	private int contador = 0;
	private int identificador;
	private String nombre;
	private int edad;
	
	public Persona(int contador, String nombre, int edad) {
		super();
		this.contador = contador;
		this.identificador = this.contador+1;
		this.nombre = nombre;
		this.edad = edad;
	}

	
	
	
	
}
