package modelo;

public class PersonaRefactoriza {

	String nombre;
	IAccion [] acciones;
	
	

	



	public PersonaRefactoriza(String nombre) {
		super();
		this.nombre = nombre;
		this.acciones = new IAccion[10];
	}







	public void identificarse() {
		System.out.println("Soy una persona y me llamo " + nombre);
	}
}
