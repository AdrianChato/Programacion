package boletin2;

public class Alumno {

	String nombre;
	String apellidos;
	String email;
	NotaAsignatura notaProg;
	NotaAsignatura notaBase;
	
	public Alumno(String nombre, String email) {
		super();
		this.nombre = nombre;
		this.email = email;
	}

	@Override
	public String toString() {
		return "Alumno [nombre=" + nombre + ", apellidos=" + apellidos + ", email=" + email + ", notaProg=" + notaProg
				+ ", notaBase=" + notaBase + "]";
	}
	
	
}
